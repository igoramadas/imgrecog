{
    "name": "imgrecog",
    "version": "2.0.0",
    "description": "Node.js tool to get tags and process images using the Google Vision, Clarifai and Sightengine APIs.",
    "homepage": "http://github.com/igoramadas/imgrecog",
    "author": "Igor Ramadas <igor@devv.com>",
    "license": "MIT",
    "subdomain": "imgrecog",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "domains": [
        "imgrecog.devv.com"
    ],
    "bin": {
        "imgrecog": "./bin/imgrecog"
    },
    "scripts": {
        "prepublishOnly": "./node_modules/.bin/tsc --removeComments",
        "docs": "make docs"
    },
    "engines": {
        "node": ">=12.0"
    },
    "dependencies": {
        "@google-cloud/vision": "^2.1.2",
        "anyhow": "^2.0.6",
        "chalk": "^4.1.0",
        "form-data": "^3.0.0",
        "yargs": "^16.2.0"
    },
    "devDependencies": {
        "@types/node": "^14.14.17",
        "@types/yargs": "^15.0.12",
        "typedoc": "^0.20.5",
        "typescript": "^4.1.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/igoramadas/imgrecog.git"
    },
    "keywords": [
        "imgrecog.js",
        "cloud vision",
        "clarifai",
        "sightengine",
        "image detection",
        "image recognition",
        "computer vision"
    ],
    "prettier": {
        "trailingComma": "none",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": false,
        "arrowParens": "always",
        "bracketSpacing": false,
        "printWidth": 250
    }
}
