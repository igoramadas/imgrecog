{
    "compilerOptions": {
        "baseUrl": "",
        "rootDir": "src",
        "outDir": "lib",
        "target": "es2018",
        "module": "commonjs",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "declaration": true,
        "skipLibCheck": true
    },
    "typedocOptions": {
        "entryPoints": ["./src/"],
        "excludeExternals": true,
        "excludePrivate": true,
        "mode": "modules",
        "name": "IMGRecog",
        "out": "docs"
    }
}
